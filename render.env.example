# Render Deployment Environment Variables
# Copy these to your Render service environment variables

# Frontend Service (neurocal-frontend)
VITE_API_URL=https://neurocal-backend.onrender.com
VITE_ANALYTICS_ENABLED=true
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_...
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Backend Service (neurocal-backend)
NODE_ENV=production
PORT=10000
FRONTEND_URL=https://neurocal-frontend.onrender.com

# Database (Render PostgreSQL)
DB_HOST=your-render-db-host.onrender.com
DB_NAME=neurocal
DB_USER=neurocal_user
DB_PASSWORD=your-db-password
DB_PORT=5432

# JWT Configuration
JWT_SECRET=your-super-secure-jwt-secret-key-here

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# AI Services
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# Email Configuration
SMTP_HOST=smtp.sendgrid.net
SMTP_PORT=587
SMTP_USER=apikey
SMTP_PASS=your_sendgrid_api_key

# Redis (if using Render Redis)
REDIS_URL=redis://your-redis-url.onrender.com:6379
